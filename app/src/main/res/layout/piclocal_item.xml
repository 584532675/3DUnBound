<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/white"
    tools:context="com.unbounded.video.com.unbounded.video.activity.PicsActivity">

    <RelativeLayout
        android:id="@+id/piclocalitem_imgrela"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >
        <ImageView
            android:id="@+id/piclocalitem_cameraiv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerInParent="true"
            android:visibility="gone"
            android:src="@mipmap/cameraicon"
            android:background="@color/white"
            />
        <ImageView
            android:id="@+id/piclocalitem_imgiv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="centerCrop"
            android:background="@color/word_six_d"
            />
    </RelativeLayout>
</RelativeLayout>
