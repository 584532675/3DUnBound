<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/word_f_345"
    >

    <RelativeLayout
        android:id="@+id/threeddfrag_rela1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/word_f_345"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <android.support.v4.widget.SwipeRefreshLayout
            android:id="@+id/threeddfrag_swiperefresh"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            >
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
        <!-- 顶部图片 -->
        <ImageView
            android:id="@+id/threeddfrag_topiv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/defaultloading_fourthreeimg"
            android:background="@color/word_six_e"
            android:scaleType="centerCrop"
            android:visibility="visible"
            />
                <!--youtube视频-->
                <RelativeLayout
                    android:id="@+id/threeddfrag_youtube_rela"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/threeddfrag_topiv"
                    android:background="@color/white"
                    android:paddingLeft="10dp"
                    >
                    <ImageView
                        android:id="@+id/threeddfrag_youtube_rela_iv"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:src="@mipmap/threeddtag"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/threeddfrag_youtube_rela_iv"
                        android:layout_marginLeft="12dp"
                        android:textColor="@color/black"
                        android:text="@string/word_free_youtube"
                        />
                    <ImageView
                        android:id="@+id/threeddfrag_youtube_tag"
                        android:layout_width="@dimen/lay_30"
                        android:layout_height="@dimen/lay_30"
                        android:layout_marginTop="@dimen/lay_12"
                        android:layout_marginBottom="@dimen/lay_12"
                        android:src="@mipmap/main_setting_enter"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:visibility="invisible"
                        />
                    <TextView
                        android:id="@+id/threeddfrag_youtube_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_toLeftOf="@+id/threeddfrag_youtube_tag"
                        android:layout_centerVertical="true"
                        android:textColor="@color/word_six_nine"
                        android:textSize="@dimen/font_13"
                        android:text="@string/word_more"
                        android:visibility="invisible"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/threeddfrag_youtube_clerViewrela"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/threeddfrag_youtube_rela"
                    android:background="@color/white"
                    >
                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/free_youtube_rv"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        />
                </RelativeLayout>
        <!-- 免费 -->
        <RelativeLayout
            android:id="@+id/threeddfrag_freerela"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/threeddfrag_youtube_clerViewrela"
            android:background="@color/white"
            android:paddingLeft="10dp"
            >
            <ImageView
                android:id="@+id/threeddfrag_freeiv"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/threeddtag"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/threeddfrag_freeiv"
                android:layout_marginLeft="12dp"
                android:textColor="@color/black"
                android:text="@string/word_free_newest"
                />
            <ImageView
                android:id="@+id/threeddfrag_freetag"
                android:layout_width="@dimen/lay_30"
                android:layout_height="@dimen/lay_30"
                android:layout_marginTop="@dimen/lay_12"
                android:layout_marginBottom="@dimen/lay_12"
                android:src="@mipmap/main_setting_enter"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:visibility="visible"
                />
            <TextView
                android:id="@+id/threeddfrag_freemoretv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_toLeftOf="@+id/threeddfrag_freetag"
                android:layout_centerVertical="true"
                android:textColor="@color/word_six_nine"
                android:textSize="@dimen/font_13"
                android:text="@string/word_more"
                android:visibility="visible"
                />
        </RelativeLayout>
            <RelativeLayout
                android:id="@+id/threeddfrag_freerecyclerViewrela"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/threeddfrag_freerela"
                android:background="@color/white"
                >
                <android.support.v7.widget.RecyclerView
                    android:id="@+id/threeddfrag_freerecyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    />
            </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/threeddfrag_new_recy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/threeddfrag_freerecyclerViewrela"
                    android:background="@color/white"
                    android:paddingLeft="10dp"
                    >
                    <ImageView
                        android:id="@+id/threeddfrag_new_view"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:src="@mipmap/threeddtag"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/threeddfrag_new_view"
                        android:layout_marginLeft="12dp"
                        android:textColor="@color/black"
                        android:text="@string/word_vip_newest"
                        />
                    <ImageView
                        android:id="@+id/threeddfrag_new_next"
                        android:layout_width="@dimen/lay_30"
                        android:layout_height="@dimen/lay_30"
                        android:layout_marginTop="@dimen/lay_12"
                        android:layout_marginBottom="@dimen/lay_12"
                        android:src="@mipmap/main_setting_enter"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:visibility="visible"
                        />
                    <TextView
                        android:id="@+id/threeddfrag_new_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_toLeftOf="@+id/threeddfrag_new_next"
                        android:layout_centerVertical="true"
                        android:textColor="@color/word_six_nine"
                        android:textSize="@dimen/font_13"
                        android:text="@string/word_more"
                        android:visibility="visible"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/threeddfrag_new_recyclerViewrela"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/threeddfrag_new_recy"
                    android:background="@color/white"
                    >
                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/threeddfrag_new_recyclerView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        />
                </RelativeLayout>
            </RelativeLayout>
            </ScrollView>
        </android.support.v4.widget.SwipeRefreshLayout>

    </RelativeLayout>
</RelativeLayout>